{
  "name": "@uscreen.de/shipit-deploy-cd",
  "version": "0.5.5",
  "description": "shipit deploy without git",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com:uscreen/shipit-deploy-cd.git"
  },
  "author": "Marcus Spiegel <spiegel@uscreen.de>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint '**/*.js' --fix",
    "test": "c8 tap",
    "test:cov": "c8 -r html -r text tap",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@uscreen.de/dev-service": "^0.11.17",
    "c8": "^7.11.3",
    "chalk": "^4.0.0",
    "path": "^0.12.7",
    "shipit-utils": "^1.4.0",
    "tap": "^16.3.0"
  },
  "peerDependencies": {
    "shipit-cli": ">=4.1.0"
  },
  "devDependencies": {
    "@uscreen.de/eslint-config-prettystandard-node": "^0.2.1",
    "husky": "^8.0.1",
    "shipit-cli": "^5.3.0",
    "sinon": "^14.0.0"
  },
  "resolutions": {
    "stream-line-wrapper": "git+https://github.com/uscreen/stream-line-wrapper.git"
  },
  "services": [
    {
      "build": "../sshd",
      "image": "uscreen.de/shipit-deploy-cd/test-sshd",
      "environment": {
        "SSH_USERS": "www:1001:1001"
      },
      "ports": [
        "2222:22"
      ],
      "volumes": [
        "../sshd/ssh_keys:/etc/ssh/keys:ro",
        "../sshd/authorized_keys/www:/etc/authorized_keys/www:ro"
      ]
    }
  ]
}
